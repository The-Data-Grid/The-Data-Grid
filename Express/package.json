{
  "name": "tdg-backend",
  "version": "1.0.0",
  "description": "Development management at UCLA Bruin Home Solutions",
  "main": "app.js",
  "scripts": {
    "dev": "node preprocess/setup.js && nodemon app.js",
    "dev-tester": "node preprocess/setup.js --postgresdb=schematester && nodemon app.js --postgresdb=schematester",
    "start": "node preprocess/setup.js --deploy && node app.js --deploy",
    "test-audit": "jest audit.test.js --detectOpenHandles --silent=false",
    "test-upload": "jest upload.test.js --detectOpenHandles --silent=false",
    "test-user": "jest user.test.js --detectOpenHandles --silent=false",
    "test-audit-force": "jest audit.test.js --detectOpenHandles --forceExit",
    "test-upload-force": "jest upload.test.js --detectOpenHandles --forceExit",
    "test-user-force": "jest user.test.js --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "setup": "node preprocess/setup.js",
    "setup:prod": "node preprocess/setup.js --deploy",
    "construct": "node ./construct/cli.js",
    "db-init": "bash ./scripts/init.sh",
    "db-make-server": "bash ./scripts/make-server.sh",
    "db-fake": "bash ./scripts/fake.sh",
    "db-status": "bash ./scripts/status.sh",
    "db-start": "bash ./scripts/start.sh",
    "db-stop": "bash ./scripts/stop.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.45.0",
    "@aws-sdk/s3-request-presigner": "^3.45.0",
    "@aws-sdk/signature-v4-crt": "^3.45.0",
    "@melgrove/type": "^1.0.0",
    "@sendgrid/mail": "^7.6.0",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.1.3",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dot": "^1.1.3",
    "dotenv": "^8.6.0",
    "excel4node": "^1.7.2",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.6.0",
    "install-cmake": "^3.5.5",
    "jest-to-match-shape-of": "^1.3.1",
    "joi": "^17.4.0",
    "lodash": "^4.17.20",
    "lru-cache": "^6.0.0",
    "memorystore": "^1.6.4",
    "multer": "^1.4.4",
    "nanoid": "^3.2.0",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "pg-promise": "^10.5.6",
    "selectize": "^0.12.6",
    "strip-json-comments": "^3.1.1",
    "tempfile": "^4.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/core": "^9.1.13",
    "@types/jquery": "^3.5.11",
    "frisby": "^2.1.3",
    "jest": "^26.6.3",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "node-mocks-http": "^1.9.0",
    "supertest": "^5.0.0"
  }
}
